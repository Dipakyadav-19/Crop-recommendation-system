x=c(1,2,3,4,5)
y=c(5,13,2)
n=length(x)
lr=0.001
b01=c()
mse1=c()
ite=1:1000
b0=0
b1=0
for(i in ite){
ypr=b0+b1*x
mse=(1/n)*sum((y-ypr)**2)
slopb0=(-2/n)*sum(y-ypr)
slopb1=(-2/n)*sum(x*(y-ypr))
b0=b0+lr*slopb0
b1=b1+lr*slopb1
b01[i]=b0
mse1[i]=mse
df=data.frame(B0=b01,MSE=mse1)
}
cat("\n iteration:",i,"b1: ",b1,"b0: ",b0,"mse: ",mse)
